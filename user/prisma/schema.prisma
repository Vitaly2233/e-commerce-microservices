generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int     @id(map: "PK_Users") @default(autoincrement())
  password String
  token    String?
  username String  @db.VarChar(100)
  email    String  @unique(map: "IX_Users_Email")
}
